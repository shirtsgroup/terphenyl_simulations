cv1: READ FILE=COLVARS.WALKER_ID VALUES=cv1 IGNORE_TIME IGNORE_FORCES
cv2: READ FILE=COLVARS.WALKER_ID VALUES=cv2 IGNORE_TIME IGNORE_FORCES
cv3: READ FILE=COLVARS.WALKER_ID VALUES=cv3 IGNORE_TIME IGNORE_FORCES
cv4: READ FILE=COLVARS.WALKER_ID VALUES=cv4 IGNORE_TIME IGNORE_FORCES

metad: METAD ...
    ARG=CV_ID
    SIGMA=CV_SIGMA
    HEIGHT=0
    BIASFACTOR=CV_BF
    PACE=100000000
    GRID_MIN=CV_MIN
    GRID_MAX=CV_MAX
    FILE=HILLS.WALKER_ID
    RESTART=YES
    TEMP=SIM_TEMP
...

PRINT ARG=cv1,cv2,cv3,cv4,metad.bias FILE=COLVARS_REWEIGHT STRIDE=1
